{
  "name": "ui-framework",
  "version": "1.0.0",
  "private": true,
  "description": "An out-of-box UI solution for enterprise applications",
  "scripts": {
    "add": "node ./scripts/add-dependencies.js",
    "postinstall": "node ./scripts/install-dependencies.js",
    "install:only": "node ./scripts/install-dependencies.js",
    "init-submodule": "node ./scripts/init-submodule.js",
    "delete-submodule": "node ./scripts/delete-submodule.js",
    "analyze": "cross-env ANALYZE=1 umi build",
    "build": "umi build",
    "build:no-example": "cross-env EXAMPLE=NONE umi build",
    "build:ui": "father-build",
    "fetch:blocks": "node ./scripts/fetch-blocks.js",
    "lint": "npm run lint:js && npm run lint:ts && npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js",
    "lint-staged:ts": "tslint",
    "lint:fix": "eslint --fix --ext .js src tests && npm run lint:style && npm run tslint:fix",
    "lint:js": "eslint --ext .js src tests",
    "lint:prettier": "check-prettier lint",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "lint:ts": "tslint -p . -c tslint.yml",
    "prettier": " check-prettier write",
    "start": "umi dev --port=60002",
    "start:prd": "cross-env PRD=true umi dev --port=60003",
    "start:no-ui": "cross-env UMI_UI=none umi dev --port=60003",
    "start:no-mock": "cross-env MOCK=none umi dev",
    "start:no-browser": "cross-env UMI_UI=none MOCK=none BROWSER=none umi dev --port=60003 ",
    "start:prd:no-browser": "cross-env PRD=true UMI_UI=none MOCK=none BROWSER=none umi dev --port=60003 ",
    "start:ui-plugin": "umi ui",
    "test": "umi test",
    "test:all": "node ./tests/run-tests.js",
    "test:component": "umi test ./src/components",
    "tslint:fix": "tslint --fix \"src/**/*.ts*\"",
    "locale": "node ./scripts/locale.js",
    "gcode": "node ./scripts/genCode.js",
    "gitconfig": "git remote add gitlab git@git.gantcloud.com:ui-team/lite-ui-framework.git && git remote add github git@github.com:yuanguandong/lite-ui-framework.git",
    "gitpush": "git push gitee master && git push github master"
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx}": "npm run lint-staged:js",
    "**/*.{js,ts,tsx,md,json,jsx,less}": [
      "npm run prettier",
      "git add"
    ],
    "**/*.{ts,tsx}": "npm run lint-staged:ts"
  },
  "peerDependencies": {
    "antd": "4.x",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "umi": "2.x || ^2.9.0-0"
  },
  "dependencies": {
    "@ant-design/icons": "2.1.1",
    "@ant-design/pro-layout": "4.5.14",
    "@antv/g6": "^3.2.4",
    "@antv/g6-plugins": "^1.0.9",
    "@antv/util": "^2.0.6",
    "add-dom-event-listener": "^1.1.0",
    "array-move": "^2.1.0",
    "classnames": "^2.2.6",
    "css-vars-ponyfill": "^2.1.2",
    "dexie": "^2.0.4",
    "dva": "^2.4.1",
    "element-resize-detector": "^1.1.15",
    "expire-cache": "^1.0.0",
    "gantd": "^1.1.2",
    "inquirer": "^6.4.0",
    "install": "^0.13.0",
    "is-json": "^2.0.1",
    "lodash-decorators": "^6.0.1",
    "memoize-one": "^5.0.4",
    "mockjs": "^1.1.0",
    "npm": "^6.11.2",
    "numeral": "^2.0.6",
    "omit.js": "^1.0.2",
    "path-to-regexp": "^3.0.0",
    "prop-types": "^15.7.2",
    "qiniu-js": "^2.5.5",
    "qs": "^6.7.0",
    "rc-animate": "^2.8.3",
    "react-container-query": "^0.11.0",
    "react-contextmenu": "^2.11.0",
    "react-copy-to-clipboard": "^5.0.1",
    "react-cropper": "^1.2.0",
    "react-document-title": "^2.0.3",
    "react-fittext": "^1.0.0",
    "react-grid-layout": "^0.16.6",
    "react-media": "^1.9.2",
    "react-media-hook2": "^1.1.2",
    "react-resize-detector": "^4.2.0",
    "react-router": "^4.3.1",
    "react-sortable-hoc": "^1.10.1",
    "react-split-pane": "^0.1.87",
    "react-zmage": "^0.8.3",
    "readline": "^1.3.0",
    "recompose": "^0.30.0",
    "redux": "^4.0.1",
    "svgo": "1.3.0",
    "umi": "^2.10.0-beta.5",
    "umi-plugin-ga": "^1.1.3",
    "umi-plugin-locale": "^2.9.0",
    "umi-plugin-pro-block": "^1.3.2",
    "umi-plugin-react": "^1.9.1",
    "umi-request": "^1.0.7",
    "use-immer": "^0.3.2",
    "wangeditor": "^3.1.1",
    "yargs-parser": "^13.1.0"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.7",
    "@types/history": "^4.7.2",
    "@types/jest": "^24.0.15",
    "@types/lodash": "^4.14.133",
    "@types/qs": "^6.5.3",
    "@types/react": "^16.8.22",
    "@types/react-document-title": "^2.0.3",
    "@types/react-dom": "^16.8.4",
    "@types/recompose": "^0.30.6",
    "antd": "^3.26.2",
    "antd-pro-merge-less": "^1.0.0",
    "antd-theme-webpack-plugin": "^1.3.0",
    "babel-eslint": "^10.0.2",
    "chalk": "^2.4.2",
    "check-prettier": "^1.0.3",
    "co": "^4.6.0",
    "co-fs": "^1.2.0",
    "cross-env": "^5.2.0",
    "cross-port-killer": "^1.1.1",
    "enzyme": "^3.10.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-compat": "^3.1.2",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-markdown": "^1.0.0",
    "eslint-plugin-react": "^7.13.0",
    "father-build": "^1.14.1",
    "gh-pages": "^2.0.1",
    "husky": "^2.3.0",
    "inquirer": "^6.5.2",
    "jest-puppeteer": "^4.2.0",
    "jsdom-global": "^3.0.2",
    "less": "^3.9.0",
    "lint-staged": "^8.2.1",
    "lnk": "^1.1.0",
    "md5-node": "^1.0.1",
    "mockjs": "^1.0.1-beta3",
    "ncp": "^2.0.0",
    "node-fetch": "^2.6.0",
    "prettier": "^1.17.1",
    "request-promise-native": "^1.0.7",
    "slash2": "^2.0.0",
    "stylelint": "^10.0.1",
    "stylelint-config-css-modules": "^1.4.0",
    "stylelint-config-prettier": "^5.2.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.1.0",
    "stylelint-order": "^3.0.0",
    "tslint": "^5.17.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-react": "^4.0.0",
    "walk": "^2.3.14",
    "walk-sync": "^2.0.2"
  },
  "resolutions": {
    "svgo": "1.3.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "checkFiles": [
    "src/**/*.js*",
    "src/**/*.ts*",
    "src/**/*.less",
    "config/**/*.js*",
    "scripts/**/*.js"
  ],
  "__npminstall_done": false
}

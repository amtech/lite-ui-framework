{
  "order": 1,
  "routes": [
    {
      "path": "/**/customlanguage",
      "name": "客制化语言",
      "nameEN": "Custom Language",
      "component": "./sysmgmt/customlanguage"
    },
    {
      "path": "/**/account",
      "nameEN": "Account",
      "redirect": "/sysmgmt/account/personal",
      "exact": true
    },
    {
      "name": "个人信息",
      "nameEN": "Personal",
      "path": "/**/account/personal",
      "component": "./sysmgmt/account/settings/_layout"
    },
    {
      "name": "修改密码",
      "nameEN": "Edit Password",
      "path": "/**/account/editpwd",
      "component": "./sysmgmt/account/settings/_layout"
    },
    {
      "name": "界面配置",
      "nameEN": "UI Config",
      "path": "/**/account/uiconfig",
      "component": "./sysmgmt/account/settings/_layout"
    },
    {
      "name": "缓存清理",
      "nameEN": "Storage Clear",
      "path": "/**/account/storage",
      "component": "./sysmgmt/account/settings/_layout"
    }
  ]
}